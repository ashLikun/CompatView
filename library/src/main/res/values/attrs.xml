<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaxheightScrollView">
        <attr name="mhsv_HeightRatio" format="reference|float" />
        <attr name="mhsv_HeightDimen" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="FrameScalLayout">
        <attr name="fsl_ratio" format="reference|float" />
        <!--按照那个值为基础-->
        <attr name="fsl_orientation">
            <enum name="width" value="0"></enum>
            <enum name="height" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="ScaleImageView">
        <attr name="sci_ratio" format="integer" />
        <!--按照那个值为基础-->
        <attr name="sci_orientation">
            <enum name="width" value="0"></enum>
            <enum name="height" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>